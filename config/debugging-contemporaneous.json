{
    "simulation_name": "debugging-contemporaneous",
    "output_hdf5": "dataset-demo.hdf5",
    "seed": 42,
    "remaster_xml": "src/remaster-template-contemporaneous.xml",
    "num_simulations": 20,
    "num_workers": 3,
    "simulation_hyperparameters": {
        "duration_range": {
            "dist": "uniform_int",
            "lower_bound": 20,
            "upper_bound": 40
        },
        "num_changes": {
            "dist": "uniform_int",
            "lower_bound": 1,
            "upper_bound": 2
        },
        "r0": {
            "dist": "lognormal",
            "LN_mean": 0.91,
            "LN_sigma": 0.1
        },
        "net_removal_rate": {
            "dist": "lognormal",
            "LN_mean": -1.80,
            "LN_sigma": 0.1
        },
        "sampling_prop": {
            "dist": "uniform",
            "lower_bound": 0.1,
            "upper_bound": 0.3
        },
        "num_temp_measurements": 10,
        "contemporaneous_sample": true
    }
}