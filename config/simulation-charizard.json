{
    "simulation-name": "sim-charizard",
    "output-hdf5": "dataset-charizard.hdf5",
    "seed": 42,
    "remaster-xml": "src/remaster-template.xml",
    "num-simulations": 11000,
    "num-workers": 5,
    "simulation-hyperparameters": {
	"duration-range": [30, 90],
	"num-changes": [1, 2],
	"shrinkage-factor": 0.25,
	"r0_bounds": [0.5, 3.0],
	"net_rem_rate_bounds": [5.0, 9.0],
	"sampling_prop_bounds": [0.05, 0.50]
    }
}
