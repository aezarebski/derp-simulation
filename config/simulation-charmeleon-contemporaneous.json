{
    "simulation-name": "sim-charmeleon-contemporaneous",
    "output-hdf5": "dataset-charmeleon-contemporaneous.hdf5",
    "seed": 42,
    "remaster-xml": "src/remaster-template-contemporaneous.xml",
    "num-simulations": 2000,
    "num-workers": 5,
    "simulation-hyperparameters": {
	"duration-range": [30, 50],
	"num-changes": [1, 2],
	"shrinkage-factor": 0.50,
	"r0_bounds": [1.0, 3.0],
	"net_rem_rate_bounds": [5.0, 9.0],
	"sampling_prop_bounds": [0.05, 0.50],
        "contemporaneous_sample": true
    }
}
