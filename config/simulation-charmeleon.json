{
    "simulation-name": "sim-charmeleon",
    "output-hdf5": "dataset-charmeleon.hdf5",
    "seed": 42,
    "remaster-xml": "src/remaster-template.xml",
    "num-simulations": 2000,
    "num-workers": 5,
    "simulation-hyperparameters": {
