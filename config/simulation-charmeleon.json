{
    "simulation_name": "sim-charmeleon",
    "output_hdf5": "dataset-charmeleon.hdf5",
    "seed": 42,
    "remaster_xml": "src/remaster-template.xml",
    "num_simulations": 2000,
    "num_workers": 5,
    "simulation_hyperparameters": {
        "duration_range": {
            "dist": "uniform_int",
            "lower_bound": 30,
            "upper_bound": 50
        },
        "num_changes": {
            "dist": "uniform_int",
            "lower_bound": 1,
            "upper_bound": 2
        },
        "r0": {
            "dist": "lognormal",
            "LN_mean": 1.0,
            "LN_sigma": 0.7
        },
        "net_removal_rate": {
            "dist": "lognormal",
            "LN_mean": -1.81,
            "LN_sigma": 0.2
        },
        "sampling_prop": {
            "dist": "beta",
            "alpha": 1.1,
            "beta": 8.0
        },
        "num_temp_measurements": 128
    }
}
