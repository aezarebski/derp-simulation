{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "simulation-name": {
      "type": "string"
    },
    "output-hdf5": {
      "type": "string"
    },
    "seed": {
      "type": "integer"
    },
    "remaster-xml": {
      "type": "string"
    },
    "num-simulations": {
      "type": "integer"
    },
    "num-workers": {
      "type": "integer"
    },
    "simulation-hyperparameters": {
      "type": "object",
      "properties": {
        "duration-range": {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            }
          ]
        },
        "num-changes": {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            }
          ]
        },
        "shrinkage-factor": {
          "type": "number"
        },
        "r0_bounds": {
          "type": "array",
          "items": [
            {
              "type": "number"
            },
            {
              "type": "number"
            }
          ]
        },
        "net_rem_rate_bounds": {
          "type": "array",
          "items": [
            {
              "type": "number"
            },
            {
              "type": "number"
            }
          ]
        },
        "sampling_prop_bounds": {
          "type": "array",
          "items": [
            {
              "type": "number"
            },
            {
              "type": "number"
            }
          ]
        },
        "contemporaneous_sample": {
          "type": "boolean"
        },
        "report-temporal-data": {
            "type": "boolean"
        },
        "num-temp-measurements": {
            "type": "integer"
        }
      },
      "required": [
        "duration-range",
        "num-changes",
        "shrinkage-factor",
        "r0_bounds",
        "net_rem_rate_bounds",
        "sampling_prop_bounds",
      ]
    }
  },
  "required": [
    "simulation-name",
    "output-hdf5",
    "seed",
    "remaster-xml",
    "num-simulations",
    "simulation-hyperparameters"
  ]
}
