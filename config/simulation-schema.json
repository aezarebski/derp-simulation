{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "simulation_name": {
      "type": "string"
    },
    "output_hdf5": {
      "type": "string"
    },
    "seed": {
      "type": "integer"
    },
    "remaster_xml": {
      "type": "string"
    },
    "num_simulations": {
      "type": "integer"
    },
    "num_workers": {
      "type": "integer"
    },
    "simulation_hyperparameters": {
      "type": "object",
      "properties": {
        "duration_range": {
          "type": "object",
          "properties": {
            "dist": {
              "type": "string"
            },
            "lower_bound": {
              "type": "string"
            },
            "upper_bound": {
              "type": "string"
            }
          },
          "required": [
            "dist"
          ]
        },
        "num_changes": {
          "type": "object",
          "properties": {
            "dist": {
              "type": "string"
            },
            "lower_bound": {
              "type": "string"
            },
            "upper_bound": {
              "type": "string"
            }
          },
          "required": [
            "dist"
          ]
        },
        "r0": {
          "type": "object",
          "properties": {
            "dist": {
              "type": "string"
            },
            "LN_mean": {
              "type": "string"
            },
            "LN_sigma": {
              "type": "string"
            }
          },
          "required": [
            "dist"
          ]
        },
        "net_removal_rate": {
          "type": "object",
          "properties": {
            "dist": {
              "type": "string"
            },
            "LN_mean": {
              "type": "string"
            },
            "LN_sigma": {
              "type": "string"
            }
          },
          "required": [
            "dist"
          ]
        },
        "sampling_prop": {
          "type": "object",
          "properties": {
            "dist": {
              "type": "string"
            },
            "lower_bound": {
              "type": "string"
            },
            "upper_bound": {
              "type": "string"
            }
          },
          "required": [
            "dist"
          ]
        },
        "contemporaneous_sample": {
          "type": "boolean"
        },
        "limited_time_sampling": {
          "type": "boolean"
        },
        "num_temp_measurements": {
            "type": "integer"
        }
      },
      "required": [
        "duration_range",
        "num_changes",
        "r0",
        "net_removal_rate",
        "sampling_prop",
        "num_temp_measurements"
      ]
    }
  },
  "required": [
    "simulation_name",
    "output_hdf5",
    "seed",
    "remaster_xml",
    "num_simulations",
    "simulation_hyperparameters"
  ]
}
